                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" v-model="goodsorderpre.preStatus" name="preStatus"
                                               value="1"/>
                                        {{$t('AuditFailure')}}
                                    </label>
                                </div>
                            </div>
                            <div class="form-group" v-if="goodsorderpre.preStatus==1">
                                <div class="col-sm-2 control-label">{{$t('AuditFailureReason')}}</div>
