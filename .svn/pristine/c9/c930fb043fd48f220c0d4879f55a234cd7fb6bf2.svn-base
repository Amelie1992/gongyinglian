                                                   name="companyStaffs"></vm-select>
                                    </div>
                                    <div class="col-sm-9" v-show="accreditRead">
                                        <input type="text" class="form-control" v-model="companyStaff.userName"
                                               disabled>
                                    </div>
                                </div>
                            </div>
