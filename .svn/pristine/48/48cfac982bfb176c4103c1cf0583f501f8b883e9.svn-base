<script type="text/javascript">
    $(".selectpicker").select2({
        allowClear: true,
        tags: true,
        width: "100%",
        placeholder: "请选择",
    });

    //日期插件
    $(".datepicker").datetimepicker({
        language:'zh-CN',
        format: 'yyyy-mm-dd',//显示格式
        weekStart: 1,
        todayHighlight: 1,//今天高亮
        minView: 'month',//设置只显示到月份
        startView:2,
        forceParse: 0,
        showMeridian: 1,
        autoclose: true,//选择后自动关闭
    });
</script>
