<body>
<div id="rrapp" v-cloak class="container-fluid">
    <section class="content-header">
        <h1>
            {{$t('DeclareOrderList')}}{{$t('Buy')}} &nbsp;<small v-if="title!=''">{{title}}</small>
        </h1>
        <ol class="breadcrumb">
            <li v-if="title==null"><a href="#" @click="reload">{{$t('refresh')}}</a></li>
            <li v-if="title=='详情'"><a href="#" @click="reload">{{$t('return')}}</a></li>
            <li v-if="title=='修改'"><a href="#" @click="reload">{{$t('return')}}</a></li>
            <li v-if="title=='新增'"><a href="#" @click="reload">{{$t('return')}}</a></li>
        </ol>
    </section>

    <div v-show="showList">
        <div class="row">
            <!-- <div class="col-xs-3">
                 <button class="btn btn-default m-left-5" @click="searchButton('1')"><i class="fa fa-search"></i>&nbsp;{{$t('FullOrder')}}</button>
                 <button class="btn btn-default m-left-5" @click="searchButton('2')"><i class="fa fa-search"></i>&nbsp;{{$t('InTheProcessing')}}</button>
                 <button class="btn btn-default m-left-5" @click="searchButton('3')"><i class="fa fa-search"></i>&nbsp;{{$t('OrderCompletion')}}</button>
             </div>-->

        </div>
        <div class="row">
            <div class="col-xs-2">
                <input type="text" class="form-control" v-model="params.orderNumber"
                       :placeholder="$t('EnquiryAccordingToOrderNumber')" maxlength="30"/>
            </div>

            <div class="col-xs-2">
                <vm-select :options="statusSelect" title="选择订单状态" search="true"
                           name="statusSelect" @input="" v-model="params.orderState"></vm-select>
            </div>

            <button class="btn btn-default m-left-5" @click="query"><i class="fa fa-search"></i>&nbsp;{{$t('query')}}
            </button>
            <button class="btn btn-primary m-left-5" @click="add"><i class="fa fa-plus"></i>&nbsp;{{$t('added')}}
            </button>
            <div class="pull-right col-xs-2"><a href="http://www.haiguan.info/OnLineSearch/Gateway/Gatewaystate.aspx">{{$t('CustomsClearanceStatusQuery')}}</a>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <table id="table"></table>
            </div>
        </div>
    </div>
    <!--新增订单-->
    <div v-show="showList1" class="panel panel-default" style="margin-top: 20px;">
        <div class="panel-body">
            <form class="form-horizontal" :model="declareOrder" id="addForm"
                  onkeydown="if(event.keyCode==13){return false;}">
                <!--//合同信息-->
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <span>{{$t('ContractInformation')}}</span>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="col-sm-5">
                                    <div class="form-group required">
                                        <label class="col-lg-3 control-label">{{$t('ContractNumber')}}</label>
                                        <div class="col-lg-6">
                                            <input id="contractNumber" type="text" class="form-control" name="contractNumber"
                                                   v-model="declareOrder.contractNumber" @change="contractWrite(this)"
                                                   :placeholder="$t('ContractNumber')" disabled/>

                                            <input type="radio" name="contractCode" value="1" checked="checked"
                                                   onchange="change2(this)"/>{{$t('AutomaticGeneration')}}
                                            &nbsp;&nbsp;<input type="radio" name="contractCode" value="0"
                                                               onchange="change2(this)"/>{{$t('ManualInput')}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!--//收货地址信息-->
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <span>{{$t('ReceiptInformation')}}</span>
                        <a href="javascript:void(0);" class="pull-right m-right-5" @click="openAddressCheck">{{$t('ChooseReceiptAddress')}}</a>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="col-sm-5">
                                    <div class="form-group required">
                                        <label class="col-lg-3 control-label">{{$t('contact')}}</label>
                                        <div class="col-lg-6">
                                            <input type="text" class="form-control" v-model="declareOrder.buyContacts"
                                                   :placeholder="$t('contact')" name="buyContacts"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="form-group required">
                                        <label class="col-lg-3 control-label">{{$t('phone')}}</label>
                                        <div class="col-lg-6">
                                            <input type="text" class="form-control" v-model="declareOrder.buyPhone"
                                                   :placeholder="$t('phone')" name="buyPhone"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="col-sm-5">
                                    <div class="form-group required">
                                        <label class="col-lg-3 control-label">{{$t('ReceivingAddress')}}</label>
                                        <div class="col-lg-6">
                                            <textarea type="text" class="form-control" v-model="declareOrder.buyAddress"
                                                      :placeholder="$t('ReceivingAddress')"
                                                      name="buyAddress"></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!--//商家信息-->
                <div class="panel panel-default m-top-10">
                    <div class="panel-heading">
                        <span>{{$t('BusinessInformation')}}</span>
                        <!--<a href="javascript:void(0);" class="pull-right m-right-5" @click="openMerchantCheck">选择商家</a>-->
                    </div>
                    <div class="panel-body">
                        <div class="row" v-show="merchantSelect">
                            <div class="col-sm-12">
                                <div class="col-sm-5">
                                    <div class="form-group required">
                                        <label class="col-lg-3 control-label">{{$t('CorporateName')}}</label>
                                        <div class="col-lg-8">
                                            <vm-select :options="companys" :title="$t('PleaseChooseSellerCompany')"
                                                       search="true"
                                                       v-model="declareOrder.merchantCompanyId" name="companyId"
                                                       :value="declareOrder.merchantCompanyId"
                                                       :method="getOffers"></vm-select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="form-group required">
                                        <label class="col-lg-3 control-label">{{$t('OfferInformation')}}</label>
                                        <div class="col-lg-8">
                                            <vm-select :options="offers" title="请选择报盘标题" search="true"
                                                       v-model="orderOfferEntity.offerId" name="offerId"
                                                       :value="orderOfferEntity.offerId"
                                                       :method="getOffer"></vm-select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" v-show="!merchantSelect">
                            <div class="col-sm-12">
                                <div class="col-sm-5">
                                    <div class="form-group">
                                        <label class="col-lg-3 control-label">{{$t('CorporateName')}}</label>
                                        <div class="col-lg-6">
                                            <input type="text" class="form-control"
                                                   v-model="declareOrder.merchantCompanyName"
                                                   :placeholder="$t('CorporateName')" :disabled="true"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="form-group">
                                        <label class="col-lg-3 control-label">{{$t('OfferInformation')}}</label>
                                        <div class="col-lg-6">
                                            <input type="text" class="form-control" v-model="declareOrder.offerName"
                                                   :placeholder="$t('OfferInformation')" :disabled="true"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="col-sm-5">
                                    <div class="form-group">
                                        <label class="col-lg-3 control-label">{{$t('contact')}}</label>
                                        <div class="col-lg-6">
                                            <input type="text" class="form-control" v-model="orderOfferEntity.contacts"
                                                   :placeholder="$t('contact')" :disabled="true"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="form-group">
                                        <label class="col-lg-3 control-label">{{$t('phone')}}</label>
                                        <div class="col-lg-6">
                                            <input type="text" class="form-control"
                                                   v-model="orderOfferEntity.contactWay"
                                                   :placeholder="$t('phone')" :disabled="true"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <div class="col-sm-5">
                                        <div class="form-group">
                                            <label class="col-lg-3 control-label">{{$t('hundredweight')}}</label>
                                            <div class="col-lg-6">
                                                <select v-model="orderOfferEntity.unit" class="form-control"
                                                        :disabled="true">
                                                    <option value="0">元/吨</option>
                                                    <option value="1">元/千克</option>
                                                    <option value="2">元/柜</option>
                                                    <option value="3">元/m³</option>
                                                    <option value="4">美元/m³</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-5">
                                        <div class="form-group">
                                            <label class="col-lg-3 control-label">{{$t('declareVotes')}}</label>
                                            <div class="col-lg-6">
                                                <input class="form-control"
                                                       style="padding-left: 10px; padding-right: 10px;"
                                                       type="number" step="1" min="1" name="quantityNew"
                                                       v-model="orderOfferEntity.quantityNew"
                                                       :placeholder="$t('declareVotes')" @change="quantityC" maxlength="10">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="col-sm-5">
                                    <div class="form-group">
                                        <label class="col-lg-3 control-label">{{$t('ChargeUnitPrice')}}</label>
                                        <div class="col-lg-6">
                                            <input class="form-control" style="padding-left: 10px; padding-right: 10px;"
                                                   type="number" step="1" min="1" name="chargeRules"
                                                   v-model="orderOfferEntity.chargeRules"
                                                   :placeholder="$t('ChargeUnitPrice')" disabled>
                                        </div>
                                    </div>
                                </div>
                                <!--<div class="col-sm-5">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">{{$t('Remarks')}}</label>
                                        <div class="col-sm-6">
                                            <textarea class="form-control" v-model="declareOrder.note" maxlength="200"
                                                      :placeholder="$t('Remarks')" name="note"></textarea>
                                        </div>
                                    </div>
                                </div>-->
                            </div>
                        </div>
                    </div>
                </div>

                <!--//货物信息-->
                <div class="panel panel-default m-top-10">
                    <div class="panel-heading">
                        <span>{{$t('GoodsInformation')}}</span>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="col-xs-2" style="padding-left: 0px;">
                                    <input id="orderNum" type="text" class="form-control" @change="addGoods('orderNum')"
                                           v-model="declareOrder.goodsNumber" name="orderNumber"
                                           :placeholder="$t('OrderNumberForGoods')" onkeyup="this.value=this.value.replace(/\s+/g,'')"/>
                                </div>
                                <button v-if="!addTxBtn&&!updateStatus" type="button" id="txtBtn" @click="addGoods('txtBtn')"
                                        class="btn btn-sm btn-primary pull-right">{{$t('NewGoods')}}
                                </button>
                            </div>
                        </div>
                        <div class="row">
                            <!--<div class="col-sm-12">-->
                                <!--<table id="goodsTab" class="table">-->

                                <!--</table>-->
                            <!--</div>-->
                            <div class="col-sm-12 table-responsive" style="height: 400px;">
                                <table class="table table-bordered">
                                    <thead>
                                    <tr>
                                        <th style="width: 10%">{{$t('ProductName')}}</th>
                                        <th style="width: 10%">{{$t('place')}}</th>
                                        <th style="width: 10%">{{$t('ProducerOrNumber')}}</th>
                                        <th style="width: 10%">{{$t('weight')}}</th>
                                        <th style="width: 10%">{{$t('unit')}}</th>
                                        <th style="width: 10%">{{$t('pack')}}</th>
                                        <th v-if="!addTxBtn&&!updateStatus1" style="width: 10%">{{$t('chaozuo')}}</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <!--手动输入-->
                                    <tr v-for="(item,index) in goods" v-if="!addTxBtn&&goods&&!updateStatus1">
                                        <td><input type="text" class="form-control" maxlength="25"
                                                   v-model="item.commodityName">{{item.commodityCountryIsName}}</td>
                                        <!--<td>
                                            <input type="text" class="form-control" maxlength="25"
                                                   v-model="item.commodityCountry">{{item.commodityCountry}}</td>-->
                                        <td v-if="item.commodityCountryIsName">
                                            <input type="text" class="form-control"
                                                   v-model="goods[index].commodityCountry">
                                        </td>
                                        <td v-if="!item.commodityCountryIsName">
                                            <select class="form-control selectpicker" data-live-search="true" data-dropup-auto="false"
                                                    v-model="goods[index].commodityCountry">
                                                <option :value="p.name" v-for="p in placeData">{{p.name}}</option>
                                            </select>
                                        </td>

                                        <td><input type="text" class="form-control" maxlength="25"
                                                   v-model="item.commodityFactoryNumber"></td>
                                        <td><input type="text" class="form-control"
                                                   v-model="item.commodityCount"></td>
                                        <td>
                                            <!--<input type="text" class="form-control"
                                                   v-model="item.commodityUnit">-->
                                            <select type="text" class="form-control"
                                                    v-model="goods[index].commodityUnit">
                                                <option :value="p.code" v-for="p in unitList">{{p.name}}</option>
                                            </select>
                                        </td>
                                        <td><input type="text" class="form-control" maxlength="25"
                                                   v-model="item.commodityPacking"></td>
                                        <td><a class="fa fa-trash-o" href="javascript:void(0)"
                                               @click="removeGoods(index)"
                                               class=""></a></td>
                                    </tr>
                                    <!--跟随系统-->
                                    <tr v-for="(item,index) in goods"
                                        v-if="addTxBtn&&!updateStatus1">
                                        <td>{{item.commodityName}}</td>
                                        <td v-if="item.commodityCountryIsName">
                                            {{item.commodityCountry}}
                                        </td>
                                        <!--<td v-if="!item.commodityCountryIsName">
                                            <select type="text" class="form-control" disabled
                                                    v-model="goods[index].commodityCountry">
                                                <option :value="p.code" v-for="p in placeData">{{p.name}}</option>
                                            </select>
                                        </td>-->
                                        <td>{{item.commodityFactoryNumber}}</td>
                                        <td>{{item.commodityCount}}</td>
                                        <td v-if="item.commodityUnit==1">吨</td>
                                        <td v-if="item.commodityUnit==2">千克</td>
                                        <td>{{item.commodityPacking}}</td>
                                        <td v-if="!addTxBtn"></td>
                                    </tr>
                                    <!--修改页面-->
                                    <tr v-for="(item,index) in goods" v-if="updateStatus1">
                                        <td>{{item.commodityName}}</td>
                                        <!--<td>{{getCommodityCountry(item.commodityCountry)}}</td>-->
                                        <td>{{item.commodityCountry}}</td>
                                        <td>{{item.commodityFactoryNumber}}</td>
                                        <td>{{item.commodityCount}}</td>
                                        <td v-if="item.commodityUnit==1">吨</td>
                                        <td v-if="item.commodityUnit==2">千克</td>
                                        <td>{{item.commodityPacking}}</td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group">
                                <label class="col-sm-2 control-label">{{$t('Remarks')}}</label>
                                <div class="col-sm-10">
                                    <input type="text" name="note" class="form-control"
                                           v-model="declareOrder.note"
                                           :placeholder="$t('Remarks')"/>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group required">
                                <label class="col-sm-2 control-label">{{$t('TotalPrice')}}</label>
                                <div class="col-sm-10 input-group">
                                    <input style="width: 20%" type="text" name="total" class="form-control"
                                           step="0.01" min="0.01"
                                           v-model="declareOrder.total"
                                           :placeholder="$t('OrderTotalAmount')">

                                    <select style="width: 12%" class="form-control"
                                            v-model="declareOrder.unit"
                                            name="unit">
                                        <option value="0">{{$t('Yuan')}}</option>
                                        <option value="1">{{$t('Dollar')}}</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-2 control-label"></div>
                        <input type="button" class="btn btn-primary"
                               @click="validate" :value="$t('sure')"/>
                        &nbsp;&nbsp;<input type="button" class="btn btn-warning" @click="reload" :value="$t('return')"/>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <!--售后处理-->
    <div class="panel panel-default" v-show="afterList">
        <section class="content-header">
            <h1>
                {{$t('AfterSaleService')}} &nbsp;<small v-if="title!=''">{{title}}</small>
            </h1>
        </section>
        <div class="panel-body">
            <form class="form-horizontal" id="afterForm" onkeydown="if(event.keyCode==13){return false;}">
                <!--申请售后的商品信息-->
                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-2 control-label">{{$t('ServiceType')}}</div>
                        <div class="col-sm-10">
                            <!--placeholder="服务类型 索赔"/>-->
                            <input type="radio" v-model="afterSale.serviceType" name="serviceType" id="radio" value="2"
                                   checked/>
                            {{$t('ClaimDemage')}}
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-2 control-label">{{$t('RefundForm')}}</div>
                        <div class="col-sm-3">
                            <select class="form-control" data-width="auto"
                                    v-model="afterSale.refundForm" name="refundForm">
                                <option value="">{{$t('PleaseChooseTheRefundMethod')}}</option>
                                <option v-for="item in refundFormList" :value="item.value">
                                    {{item.label}}
                                </option>
                            </select>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-2 control-label">{{$t('ClaimReason')}}</div>
                        <div class="col-sm-3">
                            <select class="form-control"
                                    v-model="afterSale.reason" name="reason"
                                    data-width="auto">
                                <option value="">{{$t('PleaseChooseTheReason')}}</option>
                                <option v-for="item in reasonList" :value="item.value">
                                    {{item.label}}
                                </option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-2 control-label">{{$t('ProblemDescription')}}</div>
                        <div class="col-sm-3">

                            <textarea name="description" class="form-control" rows="5"
                                      v-model="afterSale.description"></textarea>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-2 control-label">{{$t('Pictures')}}</div>
                        <div class="col-sm-3">
                            <commodity-fileinput :this_maxFileCount="6" ref="file1" :files="images1"
                                                 :image_list="afterSale.images"></commodity-fileinput>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-2 control-label"></div>
                        <input type="button" class="btn btn-primary" @click="validate2" :value="$t('sure')"/>
                        &nbsp;&nbsp;<input type="button" class="btn btn-warning" @click="reload" :value="$t('return')"/>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <!--订单详情-->
    <div v-show="showList2" style="margin-top: 20px;">
        <section class="content-header">
            <h1>
                {{$t('OrderDetails')}} &nbsp;<small v-if="title!=''">{{title}}</small>
            </h1>
        </section>
        <form class="form-horizontal" onkeydown="if(event.keyCode==13){return false;}">
            <div class="row">
                <div class="panel panel-default col-sm-6 text-center" style="height: 200px">
                    <div class="panel-body text-center" style="height: 300px">
                        <div>{{$t('ContractNumber')}}：{{declareOrder.contractNumber}}</div>
                        <div>{{$t('OrderId')}}：{{declareOrder.orderNumber}}</div>
                        <!--<div>{{$t('TotalPrice')}}：{{declareOrder.total}}</div>-->
                        <template v-if="declareOrder.orderState==0">
                            <h1>{{$t('WaitingForConfirmation')}}</h1>
                        </template>
                        <template v-if="declareOrder.orderState==1">
                            <h1>{{$t('accept')}}</h1>
                        </template>
                        <template v-if="declareOrder.orderState==2">
                            <h1 v-if="declareOrder.payState==0">{{$t('WaitForPayment')}}</h1>
                            <button v-if="declareOrder.payState==0">{{$t('ImmediatePayment')}}</button>
                            <h1 v-if="declareOrder.payState==1">{{$t('TheStorageChargeHasBeenSettled')}}</h1>
                        </template>
                        <template v-if="declareOrder.orderState==3">
                            <h1>{{$t('PendingShip')}}</h1>
                        </template>
                        <template v-if="declareOrder.orderState==4">
                            <h1>{{$t('PendingReceipt')}}</h1>
                        </template>
                        <template v-if="declareOrder.orderState==5">
                            <h1>待卖家取消</h1>
                        </template>
                        <template v-if="declareOrder.orderState==6">
                            <h1>{{$t('OrderCompletion')}}</h1>
                        </template>
                        <template v-if="declareOrder.orderState==7">
                            <h1>{{$t('OrderCancellation')}}</h1>
                        </template>
                    </div>
                </div>
                <div class="panel panel-default col-sm-6" align="center" style="height: 200px">
                    <!--v-if="order.passStatus==1"-->
                    <div class="panel-body">
                        <div>{{$t('ConsigneeInformation')}}</div>
                        <div>{{$t('consignee')}}：{{declareOrder.buyContacts}}</div>
                        <div>{{$t('address')}}：{{declareOrder.buyAddress}}</div>
                        <div>{{$t('phone')}}：{{declareOrder.buyPhone}}</div>
                    </div>
                </div>
            </div>

        </form>

        <div class="row" v-show="fileShow">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        {{$t('customsInformation')}}
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <div class="col-sm-2 control-label">{{$t('DeclarationForm')}}:</div>
                                    <div class="col-sm-10" id="declare" style="display:none">
                                        <a href="javascript:void(0)"
                                           @click="fileDownload(fileList.declare,fileList.declareName)">{{$t('downLoad')}}</a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <div class="col-sm-2 control-label">{{$t('paymentSheet')}}:</div>
                                    <div class="col-sm-10" id="scottare" style="display:none">
                                        <a href="javascript:void(0)"
                                           @click="fileDownload(fileList.scottare,fileList.scottareName)">{{$t('downLoad')}}</a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <div class="col-sm-2 control-label">{{$t('CheckList')}}:</div>
                                    <div class="col-sm-10" id="check" style="display:none">
                                        <a href="javascript:void(0)"
                                           @click="fileDownload(fileList.check,fileList.checkName)">{{$t('downLoad')}}</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <div class="col-sm-2 control-label">{{$t('QuarantineCertificate')}}:</div>
                                    <div class="col-sm-10" id="quarantine" style="display:none">
                                        <a href="javascript:void(0)"
                                           @click="fileDownload(fileList.quarantine,fileList.quarantineName)">{{$t('downLoad')}}</a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <div class="col-sm-2 control-label">{{$t('Other')}}:</div>
                                    <div class="col-sm-10" id="other" style="display:none">
                                        <a href="javascript:void(0)"
                                           @click="fileDownload(fileList.other,fileList.otherName)">{{$t('downLoad')}}</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="panel panel-default">
            <div class="panel-title text-left"><span>&nbsp;&nbsp;&nbsp;&nbsp;{{$t('business')}}：{{declareOrder.merchantCompanyName}}&nbsp;&nbsp;&nbsp;&nbsp;{{$t('ContactInformation')}}：{{declareOrder.merchantPhone}}</span>
            </div>
            <div class="panel-body">
                <div>
                    {{$t('OrderInformation')}}
                </div>
                <div class="row">
                    <div class="col-sm-3">{{$t('OrderTime')}}：{{declareOrder.createTime}}</div>
                    <div class="col-sm-3">{{$t('SingleTime')}}：{{declareOrder.orderTime}}</div>
                </div>

                <table class="table table-striped table_list contenTable" id="contenTable"></table>

                <table class="table table-striped table_list contenListTable" id="contenListTable"></table>
                <div class="row">
                    <div class="col-sm-1 text-right"><span>{{$t('Remarks')}}</span></div>
                    <div class="col-sm-8"><input type="text" class="form-control" v-model="declareOrder.note"
                                                 name="note"
                                                 :placeholder="$t('Remarks')"/></div>
                </div>
                <div class="text-right">
                    <div>{{$t('TotalPrice')}}：{{declareOrder.total}}{{declareOrder.unit == 0 ?'元':'美元'}}</div>
                    <!--<div>{{$t('freight')}}：￥0.00</div>-->
                    <!--<div>{{$t('RealPayment')}}：{{declareOrder.total}}</div>-->
                </div>
            </div>

        </div>
        <div class="row">
            <button class="btn btn-warning m-left-5" @click="returnList"><i class="fa"></i>&nbsp;{{$t('return')}}
            </button>
            <button class="btn btn-danger" style="float: right;margin-left: 10px;" v-show="complaintShow"
                    @click="addComplaint">
                <i class="fa"></i>&nbsp;{{$t('ComplaintSeller')}}
            </button>
            <button class="btn btn-primary" style="float: right;margin-left: 10px;" v-show="aftersaleShow"
                    @click="addAftersale(declareOrder)">
                <i class="fa"></i>&nbsp;{{$t('AfterSale')}}
            </button>
        </div>
    </div>

    <!--投诉-->
    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <form class="form-horizontal" id="complaintForm" onkeydown="if(event.keyCode==13){return false;}">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title" id="myModalLabel">{{$t('Complaint')}}</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="form-group">
                                <div class="col-sm-2 control-label">{{$t('ComplaintReason')}}</div>
                                <div class="col-sm-10">
                            <textarea name="reason" class="form-control" rows="8"
                                      v-model="complaint.reason"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group">
                                <div class="col-sm-2 control-label">{{$t('ComplaintVoucher')}}</div>
                                <div class="col-sm-10">
                                    <commodity-fileinput :this_maxFileCount="6" ref="file" :files="images"
                                                         :image_list="complaint.images"></commodity-fileinput>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer" v-if="!complaint.id">
                        <button type="button" class="btn btn-default" data-dismiss="modal">{{$t('cancel')}}</button>
                        <button type="button" class="btn btn-primary" @click="validateComplaint">{{$t('submission')}}
                        </button>
                    </div>
                    <div class="modal-footer" v-if="complaint.id">
                        <button type="button" class="btn btn-default" data-dismiss="modal">{{$t('close')}}</button>
                    </div>
                </form>
            </div><!-- /.modal-content -->
        </div><!-- /.modal -->
    </div>

    <div class="modal fade" id="addressModel" role="addressModel" aria-labelledby="addressLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="addressLabel">{{$t('ChooseReceiptAddress')}}</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="col-sm-2" style="padding-left: 0px;">
                                <input class="form-control" v-model="params.addressTitle" @keyup.enter="addressQuery"
                                       @blur="addressQuery" :placeholder="$t('PleaseEnterTheTitleName')"/>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <table id="addressTab"></table>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">{{$t('close')}}</button>
                    <button type="button" class="btn btn-primary" @click="getAddressDate">{{$t('sure')}}</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal -->
    </div>

</div>

