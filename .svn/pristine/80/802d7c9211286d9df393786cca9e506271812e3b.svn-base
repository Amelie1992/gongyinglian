package com.wzlue.chain.company.entity;

import java.io.Serializable;
import java.util.Date;


/**
 * 收货地址
 *
 * @author
 * @email
 * @date 2018-08-18 10:55:39
 */
public class ReceivingAddressEntity implements Serializable {
    private static final long serialVersionUID = 1L;

    //主键
    private Integer id;
    //地址标题
    private String addresshear;
    //收货人
    private String consignee;
    //所在区域
    private String region;
    //详细地址
    private String detailedaddress;
    //具体地址
    private String specificaddress;
    //手机号码
    private String phonenumber;
    //邮编
    private String zipcode;
    //默认地址
    private String defaultaddress;
    //排序
    private Integer sortnum;
    //创建人ID
    private Integer createBy;
    //公司Id
    private Integer companyId;

    /**
     * 设置：主键
     */
    public void setId(Integer id) {
        this.id = id;
    }

    /**
     * 获取：主键
     */
    public Integer getId() {
        return id;
    }

    /**
     * 设置：地址标题
     */
    public void setAddresshear(String addresshear) {
        this.addresshear = addresshear;
    }

    /**
     * 获取：地址标题
     */
    public String getAddresshear() {
        return addresshear;
    }

    /**
     * 设置：收货人
     */
    public void setConsignee(String consignee) {
        this.consignee = consignee;
    }

    /**
     * 获取：收货人
     */
    public String getConsignee() {
        return consignee;
    }

    /**
     * 设置：所在区域
     */
    public void setRegion(String region) {
        this.region = region;
    }

    /**
     * 获取：所在区域
     */
    public String getRegion() {
        return region;
    }

    /**
     * 设置：详细地址
     */
    public void setDetailedaddress(String detailedaddress) {
        this.detailedaddress = detailedaddress;
    }

    /**
     * 获取：详细地址
     */
    public String getDetailedaddress() {
        return detailedaddress;
    }

    /**
     * 设置：手机号码
     */
    public void setPhonenumber(String phonenumber) {
        this.phonenumber = phonenumber;
    }

    /**
     * 获取：手机号码
     */
    public String getPhonenumber() {
        return phonenumber;
    }

    /**
     * 设置：邮编
     */
    public void setZipcode(String zipcode) {
        this.zipcode = zipcode;
    }

    /**
     * 获取：邮编
     */
    public String getZipcode() {
        return zipcode;
    }

    public String getDefaultaddress() {
        return defaultaddress;
    }

    public void setDefaultaddress(String defaultaddress) {
        this.defaultaddress = defaultaddress;
    }

    public Integer getSortnum() {
        return sortnum;
    }

    public void setSortnum(Integer sortnum) {
        this.sortnum = sortnum;
    }

    public Integer getCreateBy() {
        return createBy;
    }

    public void setCreateBy(Integer createBy) {
        this.createBy = createBy;
    }

    public Integer getCompanyId() {
        return companyId;
    }

    public void setCompanyId(Integer companyId) {
        this.companyId = companyId;
    }

    public String getSpecificaddress() {
        return specificaddress;
    }

    public void setSpecificaddress(String specificaddress) {
        this.specificaddress = specificaddress;
    }
}
