                                </div>
                            </div>
                            <div class="form-group col-sm-4">
                                <div class="col-sm-2 control-label">{{$t('amountPaid')}}</div>
                                <div class="col-sm-10">
                                    <input type="text" name="amountPaid" class="form-control"
                                           v-model="bill.amountPaid" disabled/>
                                </div>
                            </div>
