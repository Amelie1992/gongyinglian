                            <div class="col-sm-12">
                                <div class="col-xs-2" style="padding-left: 0px;">
                                    <input id="orderNum" type="text" class="form-control" @change="addGoods('orderNum')" v-model="params.originNumber" :placeholder="$t('OrderNumber')"
                                           :readonly="true"/>
