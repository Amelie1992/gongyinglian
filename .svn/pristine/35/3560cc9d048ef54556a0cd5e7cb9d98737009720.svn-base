                <input type="text" class="form-control" v-model="params.orderNumber" placeholder="根据订单编号" maxlength="25"/>
            </div>
            <div class="col-xs-2">

                <!-- <select class="form-control selectpicker" v-model="params.orderStatus" id="paramsOrderStatus" placeholder="根据订单编号" title="选择订单状态">
                     <template v-for="item in statusSelect">
                         <option :value="item.value">{{item.label}}</option>
                     </template>
                 </select>-->

                <vm-select :options="statusSelect" title="选择订单状态" search="true" name="statusSelect" @input="" v-model="params.orderStatus"></vm-select>
            </div>

            <button class="btn btn-default m-left-5" @click="queryAuthorizes"><i class="fa fa-search"></i>&nbsp;{{$t('Inquire')}}</button>


        </div>
        <div class="row">
            <div class="col-xs-12">
                <table class="table text-nowrap" id="authorizetable">
