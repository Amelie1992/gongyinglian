                </div>
                <!--<div class="col-xs-2" style="padding-left: 0px;">
                    <input type="text" class="form-control" v-model="params.contractNumber" @keyup.enter="query" :placeholder="$t('EnterContractNumber')" maxlength="30">
                </div>-->
                <button class="btn btn-default m-left-5" @click="authorize(1)">&nbsp;{{$t('authorizeOrder')}}</button>
                <button class="btn btn-default m-left-5" @click="query"><i class="fa fa-search"></i>&nbsp;{{$t('query')}}</button>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <table id="table"></table>
            </div>
